<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yosua.aplicacionsqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Registro"
            android:label="@string/title_activity_registro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Agenda"
            android:label="@string/title_activity_perfil_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ItemContacto"
            android:label="@string/title_activity_contacto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NuevoContacto"></activity>
    </application>

</manifest>